{
  "name": "todomvc-vanilla",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node server",
    "dev": "NODE_ENV=development nodemon -w src server",
    "lint": "run-s lint-*",
    "lint-prettier": "prettier --fix src/**/*.js",
    "test": "nyc run-s test-*",
    "test-node": "blue-tape -r @std/esm src/**/*.test.js | tap-diff",
    "precommit": "run-s test",
    "pretest": "run-s lint"
  },
  "main": "index.js",
  "dependencies": {
    "@std/esm": "^0.19.1",
    "apply-html": "^1.0.0-6",
    "express": "^4.16.2",
    "koa": "^2.4.1",
    "koa-compress": "^2.0.0",
    "koa-helmet": "^3.3.0",
    "koa-mount": "^3.0.0",
    "koa-static": "^4.0.2",
    "todomvc-app-css": "^2.1.0",
    "todomvc-common": "^1.0.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "blue-tape": "^1.0.0",
    "husky": "^0.14.3",
    "nodemon": "^1.14.10",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2",
    "tap-diff": "^0.1.1"
  },
  "@std/esm": "cjs",
  "engines": {
    "node": "^8.9.4",
    "npm": "^5.6.0"
  }
}
